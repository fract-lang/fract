# Description
# ----------------
# Returns true if element is contained in array, returns false if not.
#

func Contains(array, element, start=0)
	if start < 0 | start >= len(array)
		ret false
	end

	for index in range(start, len(array))
		if array[index] == element
			ret true
		end
	end
	
	ret false
end
