var counter int32 := 1
for counter <= 1000:
  var total int32 := 0
  var ccounter int32 := 1
  for ccounter < counter:
    if counter % ccounter = 0:
      total := total + ccounter
    end
    ccounter := ccounter + 1
  end

  if total = counter:
    counter
  end

  counter := counter + 1
end
